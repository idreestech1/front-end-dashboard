{
     "version": "1.0",
     "build": [
          {
               "src": "package.json",
               "use": "@vercel/static-build",
               "config": {
                    "distDir": "dist"
               }
          }
     ],
     "routes": [
          {
               "src": "/(.*)",
               "dest": "index.html"
          }
     ]
}